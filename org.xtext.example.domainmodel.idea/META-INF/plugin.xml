<idea-plugin version="2">
	<id>org.xtext.example.domainmodel.Domainmodel</id>
	<name>Domainmodel Support</name>
	<description>
      This plugin enables smart editing of Domainmodel files.
	</description>
	<version>1.0.0</version>
	<vendor>My Company</vendor>

	<idea-version since-build="131"/>

	<extensions defaultExtensionNs="com.intellij">
		<fileTypeFactory implementation="org.xtext.example.domainmodel.idea.lang.DomainmodelFileTypeFactory"/>
		<stubElementTypeHolder class="org.xtext.example.domainmodel.idea.lang.DomainmodelFileTypeFactory"/>
		<lang.ast.factory language="org.xtext.example.domainmodel.Domainmodel" factoryClass="org.xtext.example.domainmodel.idea.DomainmodelExtensionFactory" implementationClass="org.eclipse.xtext.idea.lang.BaseXtextASTFactory"/>
      	<lang.parserDefinition language="org.xtext.example.domainmodel.Domainmodel" factoryClass="org.xtext.example.domainmodel.idea.DomainmodelExtensionFactory" implementationClass="org.xtext.example.domainmodel.idea.lang.parser.DomainmodelParserDefinition"/>
      	<lang.findUsagesProvider language="org.xtext.example.domainmodel.Domainmodel" factoryClass="org.xtext.example.domainmodel.idea.DomainmodelExtensionFactory" implementationClass="org.eclipse.xtext.idea.findusages.BaseXtextFindUsageProvider"/>
      	<lang.refactoringSupport language="org.xtext.example.domainmodel.Domainmodel" factoryClass="org.xtext.example.domainmodel.idea.DomainmodelExtensionFactory" implementationClass="org.eclipse.xtext.idea.refactoring.BaseXtextRefactoringSupportProvider"/>
      	<lang.syntaxHighlighterFactory key="org.xtext.example.domainmodel.Domainmodel" implementationClass="org.xtext.example.domainmodel.idea.lang.DomainmodelSyntaxHighlighterFactory" />
      	<stubIndex implementation="org.eclipse.xtext.psi.stubs.PsiNamedEObjectIndex"/>
      	<annotator language="org.xtext.example.domainmodel.Domainmodel" factoryClass="org.xtext.example.domainmodel.idea.DomainmodelExtensionFactory" implementationClass="org.eclipse.xtext.idea.annotation.IssueAnnotator"/>
	</extensions>

</idea-plugin>
