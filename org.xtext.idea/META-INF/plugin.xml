<idea-plugin version="2">
	<id>org.xtext.idea</id>
	<name>Xtext IDEA Core</name>
	<description>Xtext IDEA Core.</description>
	<version>1.0.0</version>
	<vendor>Eclipse Modeling Project</vendor>

	<idea-version since-build="131"/>
	
	<extensions defaultExtensionNs="com.intellij">
 	  <compileServer.plugin classpath="aopalliance.jar;org.eclipse.emf.common.jar;org.eclipse.emf.ecore.jar;org.eclipse.emf.ecore.xmi.jar;javax.inject.jar;guice.jar;guava.jar;antlr-runtime.jar;org.eclipse.xtext.jar;org.eclipse.xtext.util.jar;org.eclipse.xtext.xbase.lib.jar"/> 
   	</extensions>

	<project-components>
		<component>
			<implementation-class>org.eclipse.xtext.idea.resource.PsiFileUpdater</implementation-class>
		</component>
		<component>
			<implementation-class>org.eclipse.xtext.idea.build.XtextCompilerComponent</implementation-class>
		</component>
	</project-components>

</idea-plugin>
